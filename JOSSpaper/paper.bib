@article{tranDesigningDataCentricStudy2025,
	title = {Designing {{Data-Centric Study Protocols Guided}} by {{FAIR Principles}}},
	author = {Tran, Quynh D. and Barcelos, Erika I. and Bruckman, Laura S.},
