@article{hernandezDataIntegrationFramework2024,
	title = {A Data Integration Framework of Additive Manufacturing Based on {{FAIR}} Principles},
	author = {Hernandez, Kristen J. and Barcelos, Erika I. and Jimenez, Jayvic C. and Nihar, Arafath and Tripathi, Pawan K. and Giera, Brian and French, Roger H. and Bruckman, Laura S.},
